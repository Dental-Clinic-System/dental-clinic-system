{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2017",
      "esnext.asynciterable"
    ],
    "target": "ES2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "rootDirs": [
      "./"
    ],
    "sourceMap": true,
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true
  }
}
